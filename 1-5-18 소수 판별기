import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int N = sc.nextInt();
        char answer = 'P';
        for (int i = 2; i < N; i++) {
            if (N % i == 0) {
                answer = 'C';
                break;
            }
        }
        System.out.println(answer);
    }
}
