import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int A = sc.nextInt();
        int B = sc.nextInt();

        System.out.print(A + " ");
        while(A < B) {
            if (A % 2 == 1) {
                A *= 2;
            } else {
                A += 3;
            }
            if (A <= B) {
                System.out.print(A + " ");
            }
        }
    }
}
